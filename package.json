    "scripts": {
        "gulp": "node --harmony ./node_modules/gulp/bin/gulp",
        "server:build": "npm run gulp -- server:clean server:build",
        "server:test": "npm run gulp -- server:test",
        "server:coveralls": "cat ./coverage/server/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
        "server:climate": "cat ./coverage/server/lcov.info | node ./node_modules/codeclimate-test-reporter/bin/codeclimate.js",
        "server:coverage": "npm run server:coveralls && npm run server:climate",
        "server:start": "node --harmony ./build/index.js",
        "server:setup": "node --harmony ./build/setup.js",
        "lint": "npm run gulp -- lint",
        "test": "npm run gulp -- test",
        "coverage": "npm run server:coverage && npm run client:coverage",
        "prod_size": "npm run web:build && node ./node_modules/source-map-explorer/index.js ./public/index.js && node ./node_modules/source-map-explorer/index.js ./public/vendors.js ",
        "nsp": "npm run gulp -- nsp",
        "disable_linting": "./node_modules/.bin/cross-env DISABLE_LINTING=true",
        "enable_linting": "./node_modules/.bin/cross-env DISABLE_LINTING=false",
        "disable_coverage": "./node_modules/.bin/cross-env DISABLE_COVERAGE=true",
        "enable_coverage": "./node_modules/.bin/cross-env DISABLE_COVERAGE=false",
        "production": "./node_modules/.bin/cross-env NODE_ENV=production",
        "development": "./node_modules/.bin/cross-env NODE_ENV=development",
        "deps:check": "npm run gulp -- deps:check",
        "deps:update": "npm run gulp -- deps:update",
        "deps:upgrade": "npm run gulp -- deps:upgrade",
        "clean_install": "rm -rf ./node_modules && npm install",
        "default": "npm run gulp -- default"
    },
