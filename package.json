{
    "license": "MIT",
    "scripts": {
        "gulp": "node --harmony ./node_modules/gulp/bin/gulp",
        "server:lint": "npm run gulp -- server:lint",
        "server:build": "npm run gulp -- server:build",
        "server:build:clean": "npm run gulp -- server:clean server:build",
        "server:test": "npm run gulp -- server:test",
        "server:test:nocov": "npm run gulp -- server:test:nocov",
        "server:coveralls": "cat ./test/artifacts/server/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
        "server:climate": "cat ./test/artifacts/server/lcov.info | node ./node_modules/codeclimate-test-reporter/bin/codeclimate.js",
        "server:coverage": "npm run server:coveralls && npm run server:climate",
        "server:start": "node --harmony ./build/index.js",
        "server:setup": "node --harmony ./build/setup.js",
        "lint": "npm run gulp -- server:lint",
        "test": "npm run gulp server:test",
        "disable_linting": "./node_modules/.bin/cross-env DISABLE_LINTING=true",
        "enable_linting": "./node_modules/.bin/cross-env DISABLE_LINTING=false",
        "disable_coverage": "./node_modules/.bin/cross-env DISABLE_COVERAGE=true",
        "enable_coverage": "./node_modules/.bin/cross-env DISABLE_COVERAGE=false",
        "production": "./node_modules/.bin/cross-env NODE_ENV=production",
        "development": "./node_modules/.bin/cross-env NODE_ENV=development",
        "check_deps": "npm run gulp -- deps:check",
        "update_deps": "npm run gulp -- deps:update",
        "upgrade_deps": "npm run gulp -- deps:upgrade",
        "clean_install": "rm -rf ./node_modules && npm i"
    },
